{
	"name": "Jasmine Theme - Dark Violet",
	"type": "dark",
	"colors": {
		// lighter violet backdrop #282028
		"editor.background": "#282028",
		"tab.activeBackground": "#282028",
		// darker violet backdrop #201c20
		"panel.background": "#201c20",
		"activityBar.background": "#201c20",
		"sideBar.background": "#201c20",
		"sideBar.foreground": "#bbb",
		"sideBarSectionHeader.background": "#201c20",
		"titleBar.activeBackground": "#201c20",
		"editorGroupHeader.tabsBackground": "#201c20",
		"tab.inactiveBackground": "#201c20",
		"tab.border": "#201c20",
		// selections
		"list.activeSelectionBackground": "#513f75",
		"focusBorder": "#816baf",
		// buttons
		"button.background": "#513f75",
		// violet elements
		"statusBar.background": "#513f75", // dark violet #513f75
		"editor.selectionBackground": "#513f75",
		"activityBarBadge.background": "#816baf", // mid violet #816baf
		"progressBar.background": "#b19adf",
		"textLink.foreground": "#816baf",
		"textLink.activeForeground": "#b19adf",
		// git decoration
		"gitDecoration.addedResourceForeground": "#b19adf",
		"gitDecoration.modifiedResourceForeground": "#b19adf",
		"gitDecoration.stageModifiedResourceForeground": "#b19adf",
		"gitDecoration.untrackedResourceForeground": "#b19adf",
		"gitDecoration.conflictingResourceForeground": "#e2c08d",
		// adjust gitlens blame to be same color as comment
		"gitlens.trailingLineForegroundColor": "#777"
	},
	"tokenColors": [
		{
			"name": "Invalid",
			"scope": ["invalid"],
			"settings": {
				"foreground": "#ff5f7a"
			}
		},
		{
			"name": "Comment-like Gray Italic",
			"scope": ["comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#777"
			}
		},
		{
			"name": "White",
			"scope": [
				"punctuation.terminator",
				"punctuation.separator",
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#ccc"
			}
		},
		{
			"name": "Light Violet",
			"scope": ["string", "markup.list"],
			"settings": {
				"foreground": "#b19adf"
			}
		},
		{
			"name": "Light Violet Bold Italic",
			"scope": ["markup.heading"],
			"settings": {
				"foreground": "#b19adf",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Blue-ish",
			"scope": ["variable"],
			"settings": {
				"foreground": "#86c2f3"
			}
		},
		{
			"name": "Yellow-ish",
			"scope": ["constant", "keyword"],
			"settings": {
				"foreground": "#e4d17d"
			}
		},
		{
			"name": "Aqua-ish",
			"scope": ["meta"],
			"settings": {
				"foreground": "#54a594"
			}
		}
	]
}
